<?xml version="1.0" encoding="UTF-8"?>
<validation_rule>
  <targets>
    <target>{ItemId}</target>
  </targets>
  <conditions>
    <condition>
      <operation>Obrigatority</operation>
      <values>
        <based_on_target>{ItemId}</based_on_target>
        <value_to_trigger>{ItemValue} is checked</value_to_trigger>
      </values>
    </condition>
  </conditions>
</validation_rule>