<UserControl
    x:Class="ProjectAvalonia.Features.Project.Components.EditingBody.SolutionItem"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:editingItemBody="clr-namespace:ProjectAvalonia.Features.Project.ViewModels.EditingItemBody"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:components="clr-namespace:ProjectAvalonia.Features.Project.Components"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:CompileBindings="True"
    x:DataType="editingItemBody:SolutionItemBody"
    mc:Ignorable="d">
    <ScrollViewer>
        <components:SolutionItemContent DataContext="{Binding SolutionModel}" />
    </ScrollViewer>
</UserControl>