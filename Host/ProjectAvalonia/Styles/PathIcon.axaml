<Styles xmlns="https://github.com/avaloniaui">
    <Style Selector="PathIcon.new">
        <Setter Property="Template">
            <ControlTemplate>
                <Border Background="{TemplateBinding Background}">
                    <Path Fill="{TemplateBinding Foreground}"
                          Data="{TemplateBinding Data}"
                          Stretch="None" />
                </Border>
            </ControlTemplate>
        </Setter>
    </Style>

    <Style Selector="PathIcon.size24">
        <Setter Property="Width" Value="24" />
        <Setter Property="Height" Value="24" />
    </Style>

    <Style Selector="PathIcon.size28">
        <Setter Property="Width" Value="28" />
        <Setter Property="Height" Value="28" />
    </Style>
  
    <Style Selector="PathIcon.NotFound">
        <Setter Property="Width" Value="28" />
      <Setter Property="Data" Value="{StaticResource warning_regular}" />
    </Style>
 <Style Selector="PathIcon.Found">
        <Setter Property="Width" Value="28" />
        <Setter Property="Data" Value="{StaticResource document_regular}" />
    </Style>

</Styles>