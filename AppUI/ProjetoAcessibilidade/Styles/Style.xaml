<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">



	<!--#region Button-->

	<SolidColorBrush x:Key="ButtonBackground" Color="Transparent" />
	<SolidColorBrush x:Key="ButtonForeground" Color="#e78710" />
	<SolidColorBrush x:Key="ButtonBorderBrush" Color="#aae78710" />

	<SolidColorBrush x:Key="ButtonBackgroundPointerOver" Color="Transparent" />
	<SolidColorBrush x:Key="ButtonForegroundPointerOver" Color="#aae78710" />
	<SolidColorBrush x:Key="ButtonBorderBrushPointerOver" Color="#99e78710" />

	<SolidColorBrush x:Key="ButtonBackgroundPressed" Color="Transparent" />
	<SolidColorBrush x:Key="ButtonForegroundPressed" Color="#77e78710" />
	<SolidColorBrush x:Key="ButtonBorderBrushPressed" Color="#60e78710" />

	<SolidColorBrush x:Key="ButtonBackgroundDisabled" Color="Transparent" />
	<SolidColorBrush x:Key="ButtonForegroundDisabled" Color="#52e78710" />
	<SolidColorBrush x:Key="ButtonBorderBrushDisabled" Color="#48e78710" />
	<!--#endregion-->

</ResourceDictionary>
