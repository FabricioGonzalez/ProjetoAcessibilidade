<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="item">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="name" type="xs:string"/>
                <xs:element name="tabela">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="itemtabela" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="tipo">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration value="text"/>
                                                    <xs:enumeration value="observation"/>
                                                    <xs:enumeration value="images"/>
                                                    <xs:enumeration value="checkbox"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="topic" minOccurs="0" type="xs:string"/>
                                        <xs:element name="checkboxes" minOccurs="0">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="checkboxitem" maxOccurs="unbounded">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="topic" type="xs:string"/>
                                                                <xs:element name="opcoes">
                                                                    <xs:complexType>
                                                                        <xs:sequence>
                                                                            <xs:element name="opcao"
                                                                                        maxOccurs="unbounded">
                                                                                <xs:complexType>
                                                                                    <xs:sequence>
                                                                                        <xs:element name="checked">
                                                                                            <xs:simpleType>
                                                                                                <xs:restriction
                                                                                                        base="xs:string">
                                                                                                    <xs:enumeration
                                                                                                            value="true"/>
                                                                                                    <xs:enumeration
                                                                                                            value="false"/>
                                                                                                    <xs:enumeration
                                                                                                            value="True"/>
                                                                                                    <xs:enumeration
                                                                                                            value="False"/>
                                                                                                </xs:restriction>
                                                                                            </xs:simpleType>
                                                                                        </xs:element>
                                                                                        <xs:element name="value"
                                                                                                    type="xs:string"/>
                                                                                    </xs:sequence>
                                                                                    <xs:attribute name="item"
                                                                                                  type="xs:integer"
                                                                                                  use="required"></xs:attribute>
                                                                                </xs:complexType>
                                                                            </xs:element>
                                                                        </xs:sequence>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                                <xs:element name="texto" minOccurs="0">
                                                                    <xs:complexType>
                                                                        <xs:sequence>
                                                                            <xs:element name="topic" minOccurs="0"/>
                                                                            <xs:element name="unidade" minOccurs="0"/>
                                                                            <xs:element name="value" minOccurs="0"/>
                                                                        </xs:sequence>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                            </xs:sequence>
                                                            <xs:attribute name="item" type="xs:integer" use="required"/>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>

                                        <xs:element name="unidade" minOccurs="0"/>
                                        <xs:element name="value" minOccurs="0"/>
                                        <xs:element name="images" minOccurs="0"/>
                                    </xs:sequence>
                                    <xs:attribute name="item" type="xs:integer" use="required"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="lei">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="leiitem" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="id" type="xs:string"/>
                                        <xs:element name="descricao">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="itemdescricao" minOccurs="0"
                                                                maxOccurs="unbounded">
                                                        <xs:complexType>
                                                            <xs:simpleContent>
                                                                <xs:extension base="xs:string">
                                                                    <xs:attribute name="item" type="xs:integer"
                                                                                  use="required"/>
                                                                </xs:extension>
                                                            </xs:simpleContent>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="item" type="xs:integer" use="required"/>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>