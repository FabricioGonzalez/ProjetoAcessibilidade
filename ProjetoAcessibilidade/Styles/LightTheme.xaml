<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="BrandedPageBackgroundBrush2" Color="#E9E9E9" />

    <LinearGradientBrush x:Key="BrandedPageBackgroundBrush">
        <GradientStop Offset="0.0" Color="#ffefefef" />
        <GradientStop Offset="0.25" Color="#aaececec" />
        <GradientStop Offset="0.50" Color="#aacacaca" />
        <GradientStop Offset="0.75" Color="#aab7b7b7" />
        <GradientStop Offset="1.0" Color="#aa7c7c7c" />
    </LinearGradientBrush>

    <SolidColorBrush x:Key="NavBarBackgroundBrush" Color="#ffffff" />
    <SolidColorBrush x:Key="GridCardColor" Color="#ffffffff" />
    <SolidColorBrush x:Key="TextBoxColor" Color="#ffffff" />
    <SolidColorBrush x:Key="TextForegroundColor" Color="#000000" />
    <AcrylicBrush
        x:Key="NavBarBrush"
        FallbackColor="#ccffffff"
        TintColor="#eeffffff"
        TintLuminosityOpacity="0.3"
        TintOpacity="0.1" />
</ResourceDictionary>
