<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style TargetType="Page" x:Key="PageStyle">
        <Setter Property="Background" Value="{ThemeResource BrandedPageBackgroundBrush}" />
    </Style>    
	<Style TargetType="Grid" x:Key="RootGridStyle">
        <Setter Property="Background" Value="#20cccccc" />
    </Style>

	<ThemeShadow x:Key="ShadowElement" />


</ResourceDictionary>
